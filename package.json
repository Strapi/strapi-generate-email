{
  "name": "strapi-generate-email",
  "version": "1.5.0",
  "description": "Generate the default email features for a Strapi application.",
  "homepage": "http://strapi.io",
  "keywords": [
    "generate",
    "generator",
    "strapi"
  ],
  "main": "./lib/index.js",
  "directories": {
    "lib": "./lib"
  },
  "dependencies": {
    "fs-extra": "~0.26.2",
    "lodash": "~3.10.1",
    "nodemailer": "~1.10.0"
  },
  "devDependencies": {
    "pre-commit": "~1.1.2",
    "xo": "~0.11.2"
  },
  "xo": {
    "space": true,
    "esnext": true,
    "envs": [
      "node",
      "mocha"
    ],
    "globals": [
      "strapi",
      "angular",
      "$",
      "_"
    ],
    "ignores": [
      "node_modules/**",
      "bower_components/**",
      "coverage/**",
      "test/**"
    ],
    "rules": {
      "babel/object-shorthand": 0,
      "consistent-return": 0,
      "consistent-this": 0,
      "dot-notation": 0,
      "global-require": 0,
      "handle-callback-err": 0,
      "max-nested-callbacks": [
        1,
        6
      ],
      "no-else-return": 0,
      "no-extra-parens": 0,
      "no-implicit-coercion": 0,
      "no-invalid-this": 0,
      "no-throw-literal": 0,
      "no-undef": 0,
      "no-unused-expressions": 0,
      "padded-blocks": 0,
      "prefer-arrow-callback": 0,
      "prefer-reflect": 0,
      "prefer-template": 0,
      "quote-props": 0,
      "strict": 0
    }
  },
  "scripts": {
    "test": "xo"
  },
  "author": {
    "email": "hi@strapi.io",
    "name": "Strapi team",
    "url": "http://strapi.io"
  },
  "maintainers": [
    {
      "name": "Strapi team",
      "email": "hi@strapi.io",
      "url": "http://strapi.io"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/strapi/strapi-generate-email.git"
  },
  "bugs": {
    "url": "https://github.com/strapi/strapi/issues"
  },
  "license": "MIT"
}
